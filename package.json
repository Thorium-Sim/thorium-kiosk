{
  "name": "thorium-client",
  "version": "1.0.0",
  "description": "A client application for Thorium",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Thorium --platform=darwin --arch=x64 --overwrite"
  },
  "build": {
    "appId": "comp.alexanderson.thoriumclient",
    "mac": {
      "category": "public.app-category.entertainment",
      "target": "dmg",
      "icon": null
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "Alex Anderson <alexanderson1993@gmail.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "devDependencies": {
    "electron-packager": "^9.0.1",
    "electron-prebuilt": "^1.4.13"
  },
  "dependencies": {
    "dialogs": "^1.1.20",
    "electron-prompt": "^0.3.0",
    "smalltalk": "^2.4.1",
    "udp-discovery": "0.0.3"
  }
}