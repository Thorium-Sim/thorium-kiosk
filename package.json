{
  "name": "thorium-client",
  "version": "1.0.0",
  "description": "A client application for Thorium",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Thorium --all --overwrite --icon=icon.icns"
  },
  "build": {
    "appId": "comp.alexanderson.thoriumclient",
    "mac": {
      "category": "public.app-category.entertainment",
      "target": "dmg",
      "icon": null
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "Alex Anderson <alexanderson1993@gmail.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "devDependencies": {
    "electron-packager": "^9.0.1"
  },
  "dependencies": {
    "bonjour": "^3.5.0",
    "dialogs": "^1.1.20",
    "electron": "^1.7.11",
    "electron-json-storage": "^3.1.0",
    "electron-prompt": "^0.3.0",
    "power-off": "^1.1.2",
    "robotjs": "^0.4.7",
    "sleep-mode": "^1.1.0"
  }
}
